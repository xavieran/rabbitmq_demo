    "import send\n",
    "\n",
    "def launch_job(n):\n",
    "    client = send.RPCClient('LeChateauDL')\n",
    "    print client.call(n)\n",
    "    \n",
    "import multiprocessing\n",
    "\n",
    "manager = multiprocessing.Manager()\n",
    "pool = multiprocessing.Pool(processes=4)\n",
    "\n",
    "jobs = range(20)\n",
    "\n",
    "pool.map(launch_job, jobs)\n",
    "pool.close()\n",
    "pool.join()"
   ]
